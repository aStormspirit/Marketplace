{
  "builds": [
    {
      "src": "Marketplace/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Marketplace/wsgi.py"
    }
  ],
  "buildCommand": "python manage.py collectstatic | y"
}